{
  "name":        "workingconcept/cloudflare-craft-plugin",
  "description": "Automatically purge Craft Entry and Asset URLs in Cloudflare as they're updated. Edit",
  "version":     "1.0.0",
  "type":        "craft-plugin",
  "license":     "MIT",
  "keywords":    [
    "cms",
    "craftcms",
    "yii2"
  ],
  "authors":     [
    {
      "name":  "Mo",
      "email": "m@9dev.de"
    }
  ],
  "require":     {
    "craftcms/cms": "^3.0.0-beta.27"
  },
  "autoload":    {
    "psr-4": {
      "mattstein\\cloudflare\\": "src/"
    }
  },
  "support":     {
    "issues": "https://github.com/workingconcept/cloudflare-craft-plugin/issues?state=open",
    "source": "https://github.com/workingconcept/cloudflare-craft-plugin",
    "docs":   "https://github.com/workingconcept/cloudflare-craft-plugin",
    "rss":    "https://github.com/workingconcept/cloudflare-craft-plugin/commits/master.atom"
  },
  "extra":       {
    "name":          "Cloudflare",
    "handle":        "cloudflare",
    "schemaVersion": "1.0.0",
    "hasCpSettings": true,
    "hasCpSection":  false,
    "components":    {
      "CloudflareService": "mattstein\\cloudflare\\services\\CloudflareService",
      "RulesService":      "mattstein\\cloudflare\\services\\RulesService"
    },
    "class":         "mattstein\\cloudflare\\Cloudflare",
    "changelogUrl":  "https://raw.githubusercontent.com/workingconcept/cloudflare-craft-plugin/master/CHANGELOG.md",
    "downloadUrl":   "https://github.com/workingconcept/cloudflare-craft-plugin/archive/master.zip",
    "developer":     "Matt Stein",
    "developerUrl":  "https://workingconcept.com/"
  }
}
